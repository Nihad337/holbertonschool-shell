#!/usr/bin/env bash
# fafasffs
# shellcheck disable=SC2009


for pid in /proc/[0-9]*; do
    #wegtwetgw
    if [[ -f "$pid/comm" ]]; then
        #wetgwetgwtg
        process_name=$(cat "$pid/comm")
        
        #wegtwtgwetg
        if [[ "$process_name" == *bash* ]]; then
            #wetgwetgw
            echo "PID: $(basename "$pid") - Proses adÄ±: $process_name"
        fi
    fi
done
